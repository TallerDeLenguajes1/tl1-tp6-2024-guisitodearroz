{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build",
			"command": "dotnet",
			"type": "process",
			"args": [
				"${workspaceFolder}/tl1-tp6-2024-guisitodearroz.csproj",
				"/property: GenerateFullPaths=true",
				"/consoleloggerparameters:NoSummary;ForceNoAling"
			],
			"problemMatcher": [
				"$msCompile"
			]
		 },
			{
			"label": "publish",
			"command" :"dotnet",
			"type":"process",
			"args":[
				"publish",
				"${workspaceFolder}/tl1-tp6-2024-guisitodearroz.csproj",
				"/property:GenerateFullPaths=true",
				"/consoleloggerparameters:NoSummary;ForceNoAling"
			],
			"problemMatcher":"$msCompile"
		},
		{
			"label": "watch",
			"command":"dotnet",
			"type": "process",
			"args": [
				"watch",
				"run",
				"--project",
				"${workspaceFolder}/tl1-tp6-2024-guisitodearroz.csproj"
			],
			"problemMatcher":"$msCompile"
		 }
		]
		}
		